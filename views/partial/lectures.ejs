<div class="page-header text-center">
        <h1><span class="fa fa-anchor"></span> Mano Moduliai</h1>
  </div>
  <hr class="my-4" /> 
  
  <div class="row text-center container">
    <% lectures.forEach(function(Lectures) { %>
      <div class="col-md-4">
        <div class="card mb-4 shadow-sm">
          <div class="card-body ">
            <h2 class="card-text"> <%= Lectures.name %></h2> 
            <p class="card-text"><%= Lectures.desc %></p>
            <a class="card-text" href="lecture/<%= Lectures._id %>">Perziureti</a><br>
            <% if(user.local.userType === "admin" || user.local.userType === "lecturer") { %>
              <a class="card-text" href="editLecture/<%= Lectures._id %>">Redaguoti</a>
            <% } %>
          </div>
        </div>
      </div>
    <% }) %>
  </div>
  <hr class="my-4" />
  